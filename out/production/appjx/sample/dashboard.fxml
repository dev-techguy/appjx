<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.DashBoardController">
   <children>
      <MenuBar prefHeight="38.0" prefWidth="600.0">
        <menus>
          <Menu mnemonicParsing="false" text="Users">
            <items>
              <MenuItem mnemonicParsing="false" text="Add" />
                  <MenuItem mnemonicParsing="false" text="List" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Albums">
            <items>
              <MenuItem mnemonicParsing="false" text="Add" />
                  <MenuItem mnemonicParsing="false" text="List" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Photos">
            <items>
              <MenuItem mnemonicParsing="false" text="Add" />
                  <MenuItem mnemonicParsing="false" text="List" />
            </items>
          </Menu>
            <Menu mnemonicParsing="false" text="Help">
               <items>
                  <MenuItem fx:id="exitApp" mnemonicParsing="false" onAction="#exitMenuItemAction" text="Exit" />
               </items>
            </Menu>
        </menus>
      </MenuBar>
      <ImageView fx:id="dashImage" fitHeight="150.0" fitWidth="200.0" layoutX="232.0" layoutY="149.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/dashboard.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
